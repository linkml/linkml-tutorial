@prefix linkml_tutorial: <https://w3id.org/linkml/linkml-tutorial/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

linkml_tutorial:NamedThing a sh:NodeShape ;
    sh:closed true ;
    sh:description "The most generic type of entity that has a name" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path linkml_tutorial:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path linkml_tutorial:id ],
        [ sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path linkml_tutorial:age_in_years ],
        [ sh:description "Date on which a thing is born" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path linkml_tutorial:birth_date ] ;
    sh:targetClass linkml_tutorial:NamedThing .

linkml_tutorial:PersonCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A collection of things" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class linkml_tutorial:Person ;
            sh:description "A collection of things" ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path linkml_tutorial:entries ] ;
    sh:targetClass linkml_tutorial:PersonCollection .

linkml_tutorial:Animal a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The breed of an animal" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path linkml_tutorial:breed ],
        [ sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path linkml_tutorial:age_in_years ],
        [ sh:description "The color of an animal" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path linkml_tutorial:color ],
        [ sh:description "Date on which a thing is born" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path linkml_tutorial:birth_date ],
        [ sh:description "The weight of an animal in milligrams" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path linkml_tutorial:weight_in_mgs ],
        [ sh:description "The species of an animal" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path linkml_tutorial:species ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path linkml_tutorial:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path linkml_tutorial:id ] ;
    sh:targetClass linkml_tutorial:Animal .

linkml_tutorial:Person a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class linkml_tutorial:Animal ;
            sh:description "a collection of animals that lives with and is taken care of by a person." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path linkml_tutorial:pets ],
        [ sh:description "The main email address of a person" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path linkml_tutorial:primary_email ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path linkml_tutorial:id ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path linkml_tutorial:name ],
        [ sh:description "Date on which a thing is born" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path linkml_tutorial:birth_date ],
        [ sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path linkml_tutorial:age_in_years ],
        [ sh:description "living or dead status" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path linkml_tutorial:vital_status ] ;
    sh:targetClass linkml_tutorial:Person .


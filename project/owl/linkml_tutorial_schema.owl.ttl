@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix linkml_tutorial_schema: <https://w3id.org/linkml/linkml-tutorial-schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:linkml-tutorial-schema a owl:Ontology ;
    rdfs:label "linkml-tutorial-schema" ;
    IAO:0000700 linkml_tutorial_schema:AnimalCollection,
        linkml_tutorial_schema:NamedThing,
        linkml_tutorial_schema:PersonCollection ;
    dcterms:license "MIT" ;
    dcterms:title "linkml-tutorial-schema" ;
    rdfs:seeAlso "https://linkml.github.io/linkml-tutorial-schema" ;
    linkml:generation_date "2022-09-23T10:05:32" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "linkml_tutorial_schema.yaml" ;
    linkml:source_file_date "2022-09-23T10:05:15" ;
    linkml:source_file_size 4276 .

linkml_tutorial_schema:Breeds a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "Breeds" ;
    owl:unionOf [ rdf:rest () ] .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

linkml_tutorial_schema:AnimalCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "AnimalCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml_tutorial_schema:Animal ;
            owl:onProperty linkml_tutorial_schema:animals ] ;
    skos:definition "A collection of Animals" .

linkml_tutorial_schema:PersonCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "PersonCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml_tutorial_schema:Person ;
            owl:onProperty linkml_tutorial_schema:entries ] ;
    skos:definition "A collection of things" .

linkml_tutorial_schema:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" .

linkml_tutorial_schema:animals a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "animals" ;
    rdfs:range linkml_tutorial_schema:Animal ;
    skos:definition "A collection of animals" .

linkml_tutorial_schema:breed a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "breed" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "The breed of an animal" .

linkml_tutorial_schema:color a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "color" ;
    rdfs:range linkml:String ;
    skos:definition "The color of an animal" .

linkml_tutorial_schema:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    rdfs:range linkml_tutorial_schema:Person ;
    skos:definition "A collection of things" .

linkml_tutorial_schema:pets a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "pets" ;
    rdfs:range linkml_tutorial_schema:Animal ;
    skos:definition "a collection of animals that lives with and is taken care of by a person." .

linkml_tutorial_schema:species a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "species" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "The species of an animal" .

linkml_tutorial_schema:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range linkml_tutorial_schema:PersonStatus ;
    skos:definition "living or dead status" .

linkml_tutorial_schema:weight_in_mgs a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "weight_in_mgs" ;
    rdfs:range linkml:String ;
    skos:definition "The weight of an animal in milligrams" .

PATO:0001421 a owl:Class,
        linkml_tutorial_schema:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        linkml_tutorial_schema:PersonStatus ;
    rdfs:label "DEAD" .

schema:birthDate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:exactMatch schema:birthDate .

schema:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    rdfs:range linkml:String ;
    skos:definition "The main email address of a person" ;
    skos:exactMatch schema:email .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a person" ;
    skos:exactMatch schema:name .

linkml_tutorial_schema:Person a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Person" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml_tutorial_schema:Animal ;
            owl:onProperty linkml_tutorial_schema:pets ],
        [ a owl:Restriction ;
            owl:onClass linkml_tutorial_schema:PersonStatus ;
            owl:onProperty linkml_tutorial_schema:vital_status ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:email ],
        linkml_tutorial_schema:NamedThing ;
    skos:definition "An individual human being" ;
    skos:exactMatch <http://example.org/UNKNOWN/WIKIDATA/Q215627>,
        <http://example.org/UNKNOWN/WIKIDATA/Q5>,
        <http://example.org/UNKNOWN/biolink/Person>,
        <http://example.org/UNKNOWN/foaf/Person>,
        schema:Person .

<https://w3id.org/linkml/linkml-tutorial-schema/PersonStatus#UNKNOWN> a owl:Class,
        linkml_tutorial_schema:PersonStatus ;
    rdfs:label "UNKNOWN" .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:String ;
    skos:definition "A unique identifier for a person" ;
    skos:exactMatch schema:identifier .

linkml_tutorial_schema:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema:birthDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty linkml_tutorial_schema:age_in_years ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ] ;
    skos:definition "The most generic type of entity that has a name" ;
    skos:exactMatch schema:Thing .

linkml_tutorial_schema:Animal a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Animal" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty linkml_tutorial_schema:species ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty linkml_tutorial_schema:weight_in_mgs ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty linkml_tutorial_schema:breed ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty linkml_tutorial_schema:color ],
        linkml_tutorial_schema:NamedThing ;
    skos:altLabel "pet" ;
    skos:definition "An organism in the biological kingdom Animalia" ;
    skos:exactMatch <http://example.org/UNKNOWN/NCIT/C14182>,
        <http://example.org/UNKNOWN/WIKIDATA/Q729> ;
    skos:narrowMatch <http://example.org/UNKNOWN/FOODON/00003004> .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" .

linkml_tutorial_schema:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "PersonStatus" ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/linkml/linkml-tutorial-schema/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/linkml/linkml-tutorial-schema/PersonStatus#UNKNOWN> .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" .


